{
    "name": "pxt-pca9685",
    "version": "0.0.5",
    "description": "Generic PCA9685 driver for the BBC micro:bit",
    "license": "MIT",
    "dependencies": {
        "core": "*"
    },
    "files": [
        "driver.ts",
        "README.md"
    ],
    "testFiles": [
        "tests.ts"
    ],
    "public": true,
    "targetVersions": {
        "target": "4.0.9",
        "targetId": "microbit"
    },
    "supportedTargets": [
        "microbit"
    ],
    "preferredEditor": "tsprj",
    "yotta": {
        "config": {
            "microbit-dal": {
                "bluetooth": {
                    "open": 1,
                    "whitelist": 0,
                    "security_level": null
                }
            }
        }
    }
}
